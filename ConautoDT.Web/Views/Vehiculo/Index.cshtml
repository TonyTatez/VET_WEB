        function obtenerColores(){
            $.ajax({
                url: '@Url.Action("FindColor","Vehiculo")',
                type: "POST",
                dataType: 'json',
                success: function (result) {          
                    const selectElement = document.getElementById('colorData');
                    // Limpiar todas las opciones existentes
                    selectElement.innerHTML = '';
                    for (var i = 0; i < result.data.length; i++) {
                        $("#colorData").append('"<option  id="' + result.data[i].descripcion + '"value ="' + result.data[i].idColor + '"  style="background-color:' + result.data[i].descripcion + ' ; color:' + result.data[i].descripcion + '">' + result.data[i].descripcion + '</option>"');    
                    }
                },
                error: function (errormessage) {
                 //   alertify.warning("Chofer no encontrado. Intente de nuevo");
                }
            });
        }

