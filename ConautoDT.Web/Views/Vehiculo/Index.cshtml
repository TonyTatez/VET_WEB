        function tipoVehiculoIdentificador() {
            var elementoPlaca = document.getElementById('PlacaElement');
            var chasisPlaca = document.getElementById('ChasisElement');
            // console.log(elementoPlaca);
            
            var dropdown = document.getElementById("TipoVehiculoId");
            var descripcion = dropdown.options[dropdown.selectedIndex].text.toUpperCase();
            const listaCabezalId = document.getElementById('ListaCabezalesId');
            const descripcionCabezal = document.getElementById('descripcionCabezalId');

            const marcaChasisId = document.getElementById('descipcionChasisId');

            const marcaChasisPlaca = document.getElementById('ListaMarcaChasisId');

            if (descripcion === "CHASIS")
            {
                chasisPlaca.removeAttribute("hidden");
                elementoPlaca.setAttribute("hidden", true)
                $("#descripcionCabezalId").val("");
                $("#NumeroLlantas").val("");
                $("#NumeroLlantasfinal").val("");
                $("#TipoEstructura").val(0);
                marcaChasisId.setAttribute("required","required");
                marcaChasisPlaca.setAttribute("required", "required");
                
                listaCabezalId.removeAttribute("required");
                descripcionCabezal.removeAttribute("required");              
            }
            
            if(descripcion === "CABEZAL")
            {
                var colores = document.getElementById('colorData');
                var selectElement = document.getElementById("colorData");
                var selectedTextV2 = selectElement.options[selectElement.selectedIndex];
                if(!selectedTextV2)
                {
                    alert("No hay color de vehiculo registrado")
                }
                var selectedText = selectedTextV2.text
                colores.style.backgroundColor = selectedText;
                colores.style.color = selectedText;

                marcaChasisId.removeAttribute("required");
                elementoPlaca.removeAttribute("hidden");              
                chasisPlaca.setAttribute("hidden", true);

                marcaChasisPlaca.removeAttribute("required");
                listaCabezalId.setAttribute("required","required");
                descripcionCabezal.setAttribute("required","required");

                $("#idPlacaGenerador").val("");
                $("#descipcionChasisId").val("");
                $("#NumeroLlantasfinal").val("");
                $("#NumeroLlantas").val("");
                $("#TipoEstructura").val(0);
            }     
        }
    
