@model TMS_MANTENIMIENTO.WEB.Models.ListaLlanta
@{
    //ViewData["Title"] = "Llanta";
}





<!-- Modal Guardar-->
<div class="modal fade" id="AgregaLlantaModal" tabindex="-1" aria-labelledby="AgregaLlantaModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content" style="width:100%">
            <div class="modal-header TMSHeaderModal ">
                <h5 class="modal-title TMSTituloModal p-2">Registro de Llanta  </h5>
                <div class="d-flex align-items-end flex-column">
                    <button type="button" style="color: black" class="btn-close" data-bs-dismiss="modal" aria-label="Cerrar"></button>
                </div>
            </div>


            <form asp-action="GuardaryEditarInfo">


                 <div class="modal-body">

                    <div hidden>
                        <input type="text" id="termicoGuardar" asp-for="@Model.Termico" class="form-control form-control-lg TMSTextBoxValores" />
                    </div>

                        <div class="form-row" style="display:flex ;margin-left:50px; justify-content: center;" >
                                <div class="form-group col-md-6 ">
                                    <label style="display: block; text-align: start" for="inputGroupSelect01">Térmico</label>
                                    <input type="number" id="obtenerValor1" min="0" max="9999999" step="1" value="0" style="width:100%; padding:5px; border:0.5px solid gray ; color:red; text-align:center" disabled />
                                    @*<input type="text" id="Termico"  asp-for="@Model.Termicos" class="form-control form-control-lg TMSTextBoxValores"*@
                                </div>


                                <div style="width:100%" hidden>
                                    <h5 class="modal-title TMSTituloModal">id llanta</h5>
                                    <input type="text" id="idLlanta" asp-for="@Model.Id" style="width:100%" class="form-control form-control-lg TMSTextBoxValores" placeholder="ID LLANTA" />
                                    <h5 class="modal-title TMSTituloModal">vehiculo id</h5>
                                    <input type="text" id="ultimoGuardar" asp-for="@Model.VehiculoId" style="width:100%" class="form-control form-control-lg TMSTextBoxValores" placeholder="ULTIMA INSPECCION" />


                                </div>


                                <div class="input-group mb-4" hidden>

                                    <label class="form-label TMSlabelText" for="ruc">Ultima Inspeccion</label>
                                    <input type="number" id="obtenerValor2" min="0" max="9999999" step="1" value="0" style="width:100%; border:0.5px dotted gray ; color:green; text-align:center" disabled />


                                </div>

                            
                                <div class="form-group col-md-6">
                                    <label style="display: block; text-align: start" for="inputGroupSelect01">MM Original</label>
                                    <input type="number" id="inspeccio" asp-for="@Model.Original" min="1" max="9999999" autocomplete="off" step="any" style="width:100%; padding:5px; border:0.5px solid gray ; color: blue; text-align:center" required />
                                </div>
                            

                        </div>


                        <div class="form-row " style="display:flex ;margin-left:50px; justify-content: center;">
                                <div class="form-group col-md-6">
                                    <label  style="display: block; text-align: start" for="inputGroupSelect01">Tipo Llanta</label> 
                            
                                    @if (Model.ListaTipoLlanta != null)
                                    {

                                        @Html.DropDownListFor(m => m.TipoLlantaId,
                                    new SelectList(Model.ListaTipoLlanta, "idTipo", "descripcion", Model.TipoLlantaId),
                                    new { @class = "form-control", placeholder = "Filtar" , id="TipoLlantaId",style="text-align:center;  color:blue;  width:100%", required = "required"}
                                    )
                                    }
                                    else
                                    {

                                        <select class="custom-select" id="TipoLlantaId" style="text-align:center; color:blue; width:100%" required>
                                        </select>


                                    }
                                </div>
                                <div class="form-group col-md-6 ">
                                    <label style="display: block; text-align: start" for="inputGroupSelect01">Medida Llanta</label>

                                    @if (Model.ListaMedidaLlanta != null)
                                    {
                                        @Html.DropDownListFor(m => m.MedidaId,
                                    new SelectList(Model.ListaMedidaLlanta, "idMedida", "descripcion", Model.MedidaId),
                                    new { @class = "form-control", placeholder = "Filtar" , id="Medida",style="text-align:center;  color:blue;  ",required= "required"}
                                    )

                                    }
                                    else
                                    {
                                        <select class="custom-select" id="Medida" style=" margin-left:2%;text-align:center;  color:blue; width:100%">
                                        </select>


                                    }
                                </div>
                                
                            

                         </div>

                        <div class="form-row" style="display:flex ;margin-left:50px; justify-content: center;">
                            <div class="form-group col-md-6 ">
                                <label style="display: block; text-align: start" for="inputGroupSelect01">Diseño</label>
                                   
                                    @if (Model.ListaItemDiseno != null)
                                    {
                                        @Html.DropDownListFor(m => m.DisenoId,
                                    new SelectList(Model.ListaItemDiseno, "idDisenio", "descripcion", Model.DisenoId),
                                    new { @class = "form-control", placeholder = "Filtar" , id="disenioId",style="text-align:center;  color:blue;",required= "required"}
                                    )

                                    }
                                    else
                                    {
                                        <select class="custom-select" id="disenioId" style=" margin-left:2%;text-align:center;  color:blue; width:100%">
                                        </select>


                                    }

                             </div>
                             <div class="form-group col-md-6 ">
                                <label style="display: block; text-align: start" for="inputGroupSelect01"> Marca</label>
                                @if (Model.ListaMarca != null)
                                {
                                    @Html.DropDownListFor(m => m.MarcaId,
                                new SelectList(Model.ListaMarca, "idMarca", "descripcion", Model.MarcaId),
                                new { @class = "form-control", placeholder = "Filtar" , id="Marca",style="text-align:center;  color:blue;  ",required= "required"}
                                )
                                }
                                else
                                {
                                    <select class="custom-select" id="Marca" style=" margin-left:2%;text-align:center;  color:blue; width:100%">
                                    </select>


                                }
                            </div>
                            

                           
                        </div>



                        <div class="form-row" style="display:flex ;margin-left:50px; justify-content: center;">
                            
                            <div class="form-group col-md-6 ">
                            <label style="display: block; text-align: start" for="inputGroupSelect01">Eje  </label>
                            @if (Model.ListaEje != null)
                                {

                                    @Html.DropDownListFor(m => m.EjeId,
                                new SelectList(Model.ListaEje, "idEje", "descripcion", Model.EjeId),
                                new { @class = "form-control", placeholder = "Filtar" , id="Eje",style="text-align:center;  color:blue; ",required= "required"}
                                )
                                }
                                else
                                {
                                    <select class="custom-select" id="Eje" style=" margin-left:2%;text-align:center;  color:blue; width:100%">
                                    </select>


                                }


                            </div>
                            <div class="form-group col-md-6 ">
                                <label style="display: block; text-align: start" for="inputGroupSelect01">Estado  </label>

                                @if (Model.ListaEstado != null)
                                {
                                    @Html.DropDownListFor(m => m.EstadoId,
                                new SelectList(Model.ListaEstado, "idEstado", "descripcion", Model.EstadoId),
                                new { @class = "form-control", placeholder = "Filtar" , id="Estado",style="text-align:center; width:100%; color:blue; ",required= "required"}
                                )

                                }
                                else
                                {
                                    <select class="custom-select" id="Estado" style="text-align:center;  color:blue; width:100%">
                                    </select>


                                }
                            </div>


                        </div>



                        @*<div class="d-flex justify-content-between ">*@
                         <div class="form-row" style="display:flex ;margin-left:50px; justify-content:space-between ;">
                            
                       
                             @*</div>*@

                            <div class="form-group col-md-6 ">
                                <label style="display: block; text-align: start" for="inputGroupSelect01">Posicion Actual</label>
                                <select class="custom-select" id="colorData1" asp-for="@Model.PosicionAtual" onchange="cambiarColor1();" style="text-align:center;  color:blue; width:100%" >
                                     </select>
                            </div>

                             <div class="form-group col-md-6">
                                <label for="inputGroupSelect01">Tipo Vehiculo</label>


                                @if (Model.ListaTipoVehiculo != null)
                                {
                                    @Html.DropDownListFor(m => m.TipoVehiculoId,
                                new SelectList(Model.ListaTipoVehiculo, "idTipo", "descripcion", Model.TipoVehiculoId),
                                new { @class = "form-control", placeholder = "Filtar" , id="TipoVehiculo",onchange = "tipoVehiculos();",style="text-align:center;  color:blue;  width:100%", required = "required"}
                                )

                                }
                                else
                                {
                                    <select class="custom-select" id="TipoVehiculo" style="text-align:center;  color:blue; width:100%">
                                    </select>


                                }
                            </div>



                         </div>


                        <div id="PlacaElement1" hidden>

                             <div class="form-row" style="display:flex ;margin-left:50px; justify-content:space-between ;">
                                <div class="form-group col-md-12 ">
                                    <label style="display: block; text-align: start " for="inputGroupSelect01">Cabezal</label>

                                    @if (Model.ListaCabezalTipo != null)
                                    {

                                        @Html.DropDownListFor(m => m.CabezalId,
                                    new SelectList(Model.ListaCabezalTipo, "cabezalId", "placa", Model.CabezalId),
                                    new { @class = "form-control", placeholder = "Filtar" ,onchange = "cabezalCambio();", id="CabezalIdFinal",style="text-align:center;  color:blue;  width:100%"}
                                    )
                                    }
                                    else
                                    {


                                        <select class="custom-select" id="CabezalIdFinal" style=" margin-left:2%;text-align:center; padding: 5px; color:blue; width:100%" required>
                                        </select>


                                    }
                                </div>

                            </div>
                        </div>



                        <div id="ChasisElement1" hidden>


                            <div class="form-row" style="display:flex ;margin-left:50px; justify-content: center;">
                                <div class="form-group col-md-12 ">
                                    <label style="display: block; text-align: start " for="inputGroupSelect01">Chasis</label>

                                    @if (Model.ListaChasisTipo != null)
                                    {
                                        @Html.DropDownListFor(m => m.ChasisId,
                                    new SelectList(Model.ListaChasisTipo, "chasisId", "placa", Model.ChasisId),
                                    new { @class = "form-control", placeholder = "Filtar" , id="ChasisIdFinal" ,onchange = "chasisCambio();",style="text-align:center;  color:blue;  width:100%"}
                                    )
                                    }
                                    else
                                    {
                                        <select class="custom-select" id="ChasisIdFinal" style="text-align:center;  color:blue; width:100%" required>
                                        </select>


                                    }
                                </div>

                            </div>

                        </div>



                 </div>
                

                        <div class="modal-footer TMSHeaderModal">
                            <button id="guardarInformacion" type="submit" class="btn btn-success bi bi-save btn-grad btn-sm"> Guardar </button>
                        </div>
             </form>
        </div>
    </div>
</div>





<div class="row" style="font-size: 13px !important;">
    <div class="col-md-12">
        <div class="widget">
            <header class="widget-header" style="padding-bottom: 5px !important;">
                <div class="row">
                    <h4 class="widget-title col-md-12" style="font-weight:400 !important;font-size: 21px; color:#333333; font-family: Calibri, Helvetica Neue, Helvetica, Arial, sans-serif;">Registro de Llantas</h4>
                </div>
            </header>
            <hr class="widget-separator" style="margin-top:2px !important">
            <div class="widget-body">
                <div class="btn-group" role="group" aria-label="Basic mixed styles example" style="margin-bottom:5px !important;">
                    <button id="btnNuevo" type="button" class="btn btn-success btn-sm bi bi-clipboard-plus" data-bs-toggle="modal" data-bs-target="#AgregaLlantaModal">   Nuevo </button>

                </div>
                <div class="table-responsive">
                    <table class="table table-bordered table-sm  dataTable" id="tablaLlantas">

                        <thead>
                            <tr>
                            <tr>
                                <th >
                                    Id
                                </th>
                                <th >
                                    Termico
                                </th>
                                <th >
                                    Diseno
                                </th>
                                <th >
                                    Medida Llanta
                                </th>
                                <th >
                                    MM Original
                                </th>
                                <th >
                                    Marca
                                </th>
                                <th >
                                    Eje
                                </th>
                                <th>
                                    Estado
                                </th>
                                <th>
                                    Tipo Llanta
                                </th>
                                <th >
                                    Ultima Inspeccion
                                </th>
                                <th >
                                    Vehiculo Asignado
                                </th>
                                <th>
                                    Posicion Llanta
                                </th>
                                <th >
                                    Acciones
                                </th>
                           
                            </tr>
                        </thead>
                        <tbody>

                            @if (Model.ItemLlanta != null)
                            {

                                @foreach (var item in Model.ItemLlanta)
                                {
                                      @Html.HiddenFor(model => item.idCabezal)
                                    @Html.HiddenFor(model => item.idEstado)

                                    
                                      @Html.HiddenFor(model => item.idChasis)
                                    @Html.HiddenFor(model => item.idMedida)
                                    
                                    @Html.HiddenFor(model => item.idMarca)
                                     @Html.HiddenFor(model => item.posicion)
                                  

                                    @Html.HiddenFor(model => item.idEje)

                                    @Html.HiddenFor(model => item.idTipoVehiculo)
                                    @Html.HiddenFor(model => item.idVehiculo)
                                      @Html.HiddenFor(model => item.idMarcaCabezal)
                                    @Html.HiddenFor(model => item.idMarcaChasis)
                                                                
                                
                                   
                                    @Html.HiddenFor(model => item.idTipoLlanta)
                                    
                                    <tr>


                                        <td>
                                            @Html.DisplayFor(model => item.idLlantas)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => item.termico)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => item.diseño)
                                        </td>

                                        <td>
                                            @Html.DisplayFor(model => item.medidas)
                                        </td>

                                        <td>
                                            @Html.DisplayFor(model => item.mmOriginal)
                                        </td>

                                        <td>
                                            @Html.DisplayFor(model => item.marcas)
                                        </td>


                                        <td>
                                            @Html.DisplayFor(model => item.eje)
                                        </td>
                                        
                                        <td>
                                            @Html.DisplayFor(model => item.estado)
                                        </td>
                                        
                                        <td>
                                            @Html.DisplayFor(model => item.tipoLlanta)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => item.fechaUltimaInspeccion)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => item.vehiculo)
                                        </td>
                                          <td>
                                            @Html.DisplayFor(model => item.posicionActual)
                                        </td>


                                        <td>
                                            <button title="Editar Llanta" class="btn p-v-sm btn-success btn-sm" style="margin-right: 2px !important;" data-bs-toggle="modal" data-bs-target="#AgregaLlantaModal" onclick="editar({IdLlanta:parseInt(@item.idLlantas) , termico:'@item.termico',tipoLlanta:parseInt(@item.idTipoLlanta) ,  ultima:'@item.fechaUltimaInspeccion' , original:'@item.mmOriginal'  , vehiculoId: parseInt(@item.idVehiculo) , disenoId: parseInt(@item.idDisenio) , marcasId: parseInt(@item.idMarca)   ,medidaId : parseInt(@item.idMedida)  , ejeId: parseInt(@item.idEje) , idCabezal:parseInt(@item.idCabezal) , idChasis:parseInt(@item.idChasis) , idEstado:parseInt(@item.idEstado) , posLlante:parseInt(@item.posicionActual) , idTipoVehulo: parseInt(@item.idTipoVehiculo) , idLlanta:parseInt(@item.idLlantas)  });">
                                                <i class="bi bi-pen"></i>
                                            </button>

                                            <button title="Agregar Inspeccion Llanta" id="InspeccionLlantaIdsObtener" class="btn p-v-sm btn-primary btn-sm" style="margin-right: 2px !important;" data-bs-toggle="modal" data-bs-target="#AgregaInspeccionLlantaModal" onclick="inspeccionLineal({ placa : '@item.vehiculo' , tipoLlanta: parseInt(@item.idTipoLlanta)   , marca:parseInt(@item.idMarca)  , marcaAnterior:parseInt(@item.marcaAnterior) , marcaSiguiente:parseInt(@item.marcaSiguiente) , disenio:parseInt(@item.idDisenio) , medida:parseInt(@item.idMedida) , eje:parseInt(@item.idEje) ,posicion:'@item.posicionActual' , original:'@item.mmOriginal'  ,idVehiculo:parseInt(@item.idVehiculo) , tipoVehiculo:parseInt(@item.idTipoVehiculo) , idLlantas: parseInt(@item.idLlantas)  , idmarcachasis: parseInt(@item.idMarcaChasis) , idmarcacabezal:parseInt(@item.idMarcaCabezal)  , termico:parseInt(@item.termico) , estadoLlanta:'@item.estado', idestado:parseInt(@item.idEstado) , idTipoVehulo: parseInt(@item.idTipoVehiculo) , idCabezales: parseInt(@item.idCabezal), idChasis: parseInt(@item.idChasis) });">
                                                <i class="bi   bi-calendar2-check"></i>
                                            </button>

                                            @if(item.tipoLlanta =="REENCAUCHE"){

                                                
                                            <button title="Agregar Reencauche Llanta" class="btn p-v-sm btn-info btn-sm" style="margin-right: 2px !important;" data-bs-toggle="modal" data-bs-target="#AgregarReencauche" onclick="reencaucheInspeccion({idLlanta:parseInt(@item.idLlantas) ,  disenio:parseInt(@item.idDisenio)  , marcaAnterior:parseInt(@item.marcaAnterior)  , medida:parseInt(@item.idMedida) , posicion:'@item.posicionActual' , marcasId: parseInt(@item.idMarca) , posicion:'@item.posicion', numeroPosicion:'@item.posicionActual' , idVehiculo:parseInt(@item.idVehiculo)  , termico:'@item.termico' , original:'@item.mmOriginal' ,  fechaInspeccion:'@item.fechaUltimaInspeccion' , tipoLlantaId:parseInt(@item.idTipoLlanta) , estadoLlanta:parseInt(@item.idEstado) , idEje:parseInt(@item.idEje) , idTipoVehiculo:parseInt(@item.idTipoVehiculo)  });" disabled>

                                                <i class="bi   bi-fan"></i>
                                            </button>

                                            }else{


                                            <button title="Agregar Reencauche Llanta" class="btn p-v-sm btn-info btn-sm" style="margin-right: 2px !important;" data-bs-toggle="modal" data-bs-target="#AgregarReencauche" onclick="reencaucheInspeccion({idLlanta:parseInt(@item.idLlantas) ,  disenio:parseInt(@item.idDisenio)  , marcaAnterior:parseInt(@item.marcaAnterior)  , medida:parseInt(@item.idMedida) , posicion:'@item.posicionActual' , marcasId: parseInt(@item.idMarca) , posicion:'@item.posicion', numeroPosicion:'@item.posicionActual' , idVehiculo:parseInt(@item.idVehiculo)  , termico:'@item.termico' , original:'@item.mmOriginal' ,  fechaInspeccion:'@item.fechaUltimaInspeccion' , tipoLlantaId:parseInt(@item.idTipoLlanta) , estadoLlanta:parseInt(@item.idEstado) , idEje:parseInt(@item.idEje) , idTipoVehiculo:parseInt(@item.idTipoVehiculo) ,  tipollanta: '@item.tipoLlanta' });">

                                                <i class="bi   bi-fan"></i>
                                            </button>

                                            }


                                            <button title="Eliminar Llanta" class="btn p-v-sm btn-danger btn-sm" id="btnEliminar" data-bs-toggle="modal" data-bs-target="#eliminarModal" onclick="eliminar({idLlantas:@item.idLlantas});">
                                                <i class="bi bi-trash"></i>
                                            </button>


                                        </td>
                                    </tr>
                                }



                            }
                            else
                            {
                             <tr>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                                    <td></td>
                            <td>No hay Datos que Mostrar</td>
                            <td></td>
                                    <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                                </tr>
                            }

                        </tbody>
                        <tfoot>
                            <tr>
                                <th>
                                    Id
                                </th>
                                <th>
                                    Termico
                                </th>
                                <th>
                                    Diseno
                                </th>
                                <th>
                                    Medida Llanta
                                </th>
                                <th>
                                    MM Original
                                </th>
                                <th>
                                    Marca
                                </th>
                                <th>
                                    Eje
                                </th>
                                <th>
                                   Estado
                                </th>
                                <th>
                                   Tipo Llanta
                                </th>
                                <th>
                                    Ultima Inspeccion
                                </th>
                                <th>
                                    Vehiculo Asignado
                                </th>
                                <th>
                                    Posicion Llanta
                                </th>
                                <th>
                                    Acciones
                                </th>
                            </tr>
                        </tfoot>
                    </table>
                </div>               
            </div>
        </div>
        <br />
        <br />
    </div>
</div>



<!-- Modal Eliminar -->
<div class="modal fade" id="eliminarModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content " style="width:100%">
            <div class="modal-header TMSHeaderModal">
                <h5 class="modal-title TMSTituloModal">TMS Mantenimiento</h5>
                <div class="d-flex align-items-end flex-column">
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Cerrar"></button>
                </div>
            </div>
            <form asp-action="DeleteInformacion">


                <div class="modal-body ">
                    <div class="form-outline mb-4" hidden>
                        <label class="form-label btn-color-info TMSlabelText">Id</label>
                        <input type="text" asp-for="@Model.Id" id="idLlantaEliminar" class="form-control form-control-lg  TMSTextBoxValores" />
                    </div>
                    <div class="form-outline mb-4">
                        <label class="form-label btn-color-info TMSlabelText" style="margin-top:28px; font-size:13px">¿Está seguro de querer eliminar este Vehiculo?</label>
                    </div>
                </div>

                <div class="modal-footer TMSHeaderModal">
                    <button id="eliminarInfoData" style="padding:10px; width:50px;" type="submit" class=" btn btn-success  btn-sm btn-grad">Sí </button>
                    <button id="eliminarInfoDatos" style="padding:10px; width:50px;" data-bs-dismiss="modal" aria-label="Cerrar" type="button" class=" btn btn-danger  btn-sm btn-grad">No </button>
                </div>
            </form>
        </div>
    </div>
</div>





<!-- Modal OK -->
<div class="modal fade"  id="AgregaInspeccionLlantaModal" aria-labelledby="AgregaInspeccionLlantaModalLabel" aria-hidden="true">
    <div class="modal-dialog  modal-lg">
