<!-- Modal Vehiculo -->

<div class="modal fade" id="AgregVehiculoModal" aria-labelledby="AgregVehiculoModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content " style="height:auto">
            <div class="modal-header TMSHeaderModal">
                <h5 class="modal-title TMSTituloModal">Montaje de Llantas</h5>
                <div class="d-flex align-items-end flex-column">
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Cerrar"></button>
                </div>
            </div>
            <div hidden>
                  <input type="text" id="TipoVehiculoFinal" style="width:100%;  color:darkblue ; text-align:center ; font-size:16px" class="form-control form-control"
                                />

            </div>

            <div class="modal-body" style="height:auto;">
                <div class="form-group row mb-3 ">
                    <div class="form-group col-md-6">
                        <label for="inputGroupSelect01">Tipo Vehiculo</label>
                   
                   
                        @if (Model.ListaTipoVehiculo != null)
                        {
                            @Html.DropDownListFor(m => m.TipoVehiculoId,
                            new SelectList(Model.ListaTipoVehiculo, "idTipo", "descripcion", Model.TipoVehiculoId),
                            new { @class = "form-control", placeholder = "Filtar" , id="TipoVehiculo",onchange = "tipoVehiculos();",style="text-align:center;  color:blue;  width:100%", required = "required"})}
                        else
                        {
                            <select class="custom-select" id="TipoVehiculo" style="text-align:center;  color:blue; width:100%">
                            </select>
                        }
                    </div>
                </div>

                <div style="width:100%;" id="PlacaElement1" hidden>

                    <div class="form-group row mb-3 ">
                        <div class="form-group col-md-6">
                            <label  for="inputGroupSelect01">Cabezal</label>
                       

                            @if (Model.ListaCabezalTipo != null)
                            {
                                @Html.DropDownListFor(m => m.cabezalId,
                                new SelectList(Model.ListaCabezalTipo, "cabezalId", "placa", Model.cabezalId),
                                new { @class = "form-control", placeholder = "Filtar" ,onchange = "cabezalCambio();", id="CabezalId",style="margin-left:1%;text-align:center;  color:blue;  width:100%"})
                            }
                            else
                            {
                                <select class="custom-select" id="CabezalId" style=" margin-left:1%;text-align:center; padding: 5px; color:blue; width:100%" required>
                                </select>
                            }
                        </div>
                    </div>
                </div>


                <div style="  width:100% ; " id="ChasisElement1" hidden>
                   <div class="form-group row mb-3 ">
                        <div class="form-group col-md-6">
                            <label  for="inputGroupSelect01">Chasis</label>
                        
                            @if (Model.ListaChasisTipo != null)
                            {
                                @Html.DropDownListFor(m => m.chasisId,
                                new SelectList(Model.ListaChasisTipo, "chasisId", "placa", Model.chasisId),
                                new { @class = "form-control", placeholder = "Filtar" , id="ChasisId" ,onchange = "chasisCambio();",style="margin-left:1%;text-align:center;  color:blue;  width:100%"})
                            }
                            else
                            {
                                <select class="custom-select" id="ChasisId" style=" margin-left:1%;text-align:center;  color:blue; width:100%" required>
                                </select>
                            }
                        </div>
                    </div>
                </div>


                <div class="form-group row mb-3">
                    <div class="form-group col-md-6 ">
                        <label style="display: block; text-align: start" for="ruc">Tipo de estructura</label>

                        <input type="text" id="TipoEstructura" style="width:100%;  color:darkblue ; text-align:center ; font-size:16px" class="form-control form-control"
                               disabled />
                    </div>
                

               

                     <div class="form-group col-md-6 ">
                        <label style="display: block; text-align: start" for="ruc">Numero de Llantas</label>
                  
                        <input type="text" id="NumeroLlantasfinal" style="width:100%;  color:darkblue ; text-align:center ; font-size:16px" class="form-control form-control"
                               disabled />
                    </div>
                </div>

                <div class="modal-footer TMSHeaderModal">
                    @*<button title="Detalle de cada llanta" class="btn p-v-sm btn-primary btn-sm" style="margin-right: 2px !important;" data-bs-toggle="modal" data-bs-target="#ModalListaInspeccionLlanta" onclick="InspeccionLlanta();"> Rotación de Llantas </button>*@
