                </div>
            </div>

            <div class="modal-body ">


                <label class="form-label TMSlabelText" for="ruc" style="width:100% ; font-size:25px;">Información de la Llanta <span id="colocarDatos" style="font-size:25px;"></span> </label>

                <img asp-for="@Model.image" style="position:absolute; margin-left:-89%;margin-top:50px;  height:100px; width:50px;" src="~/imagenes/llantaDIseño.png" />

                <div hidden>
                    <input type="text" id="idLlantaEditar" class="form-control form-control-lg TMSTextBoxValores" />
                    <input type="text" id="termicoEditar" class="form-control form-control-lg TMSTextBoxValores" />
                </div>
                <div class="form-outline mb-4" style="margin-left:20%">

                    <label class="form-label TMSlabelText" for="ruc">Térmico</label>
                    <input type="number" id="obtenerValor3" min="0" max="9999999" step="1" style="width:50%; margin-left:55px; border:0.5px dotted gray ; color:red; text-align:center" disabled />
                    @*<input type="text" id="Termico"  asp-for="@Model.Termicos" class="form-control form-control-lg TMSTextBoxValores"*@

                </div>

                <div hidden style="width:100%">
                    <input type="text" id="ultimoEditar" style="width:100%" class="form-control form-control-lg TMSTextBoxValores" />
                </div>


                <div class="form-outline mb-4" style="margin-left:20%">

                    <label class="form-label TMSlabelText" for="ruc">Ultima Inspeccion</label>
                    <input type="text" id="obtenerValor4" style="width:50%; border:0.5px dotted gray ; color:green; text-align:center" disabled />


                </div>


                <div class="form-outline mb-4" style="margin-left:20%">
                    <label class="form-label TMSlabelText" for="ruc">MM Original</label>
                    <input type="text" id="originalEditar" autocomplete="off" step="any" style="width:50%;margin-left:9%; border:0.5px dotted gray ; color: blue; text-align:center" disabled />
                </div>

                <div class="form-outline mb-4" style="margin-left:20%">
                    <label class="form-label TMSlabelText" for="ruc">Marca</label>
                    <input type="text" id="marcalEditar" autocomplete="off" step="any" style="width:50%;margin-left:17%; border:0.5px dotted gray ; color: blue; text-align:center" disabled />
                </div>

                <div class="form-outline mb-4" style="margin-left:20%">
                    <label class="form-label TMSlabelText" for="ruc">Medida</label>
                    <input type="text" id="medidaEditar" autocomplete="off" step="any" style="width:50%;margin-left:15%; border:0.5px dotted gray ; color: blue; text-align:center" disabled />
                </div>

                <div class="form-outline mb-4" style="margin-left:20%">
                    <label class="form-label TMSlabelText" for="ruc">Vehiculo</label>
                    <input type="text" id="vehiculoEditar" autocomplete="off" step="any" style="width:50%;margin-left:12%; border:0.5px dotted gray ; color: blue; text-align:center" disabled />
                </div>
                <div class="form-outline mb-4" style="margin-left:20%">
                    <label class="form-label TMSlabelText" for="ruc">Eje </label>
                    <input type="text" id="EjeEditar" autocomplete="off" step="any" style="width:50%;margin-left:20%; border:0.5px dotted gray ; color: blue; text-align:center" disabled />
                </div>

                <div class="form-outline mb-4" style="margin-left:20%">
                    <label class="form-label TMSlabelText" for="ruc">Tipo de llanta </label>
                    <input type="text" id="TipoLLantaEditar" autocomplete="off" step="any" style="width:50%;margin-left:20%; border:0.5px dotted gray ; color: blue; text-align:center" disabled />
                </div>

                <div class="form-outline mb-4" style="margin-left:20%">
                    <label class="form-label TMSlabelText" for="ruc">Diseño </label>
                    <input type="text" id="DisenoLlantaEditar" autocomplete="off" step="any" style="width:50%;margin-left:20%; border:0.5px dotted gray ; color: blue; text-align:center" disabled />
                </div>

                <div class="form-outline mb-4" style="margin-left:20%">
                    <label class="form-label TMSlabelText" for="ruc">Estado </label>
                    <input type="text" id="estadoLlantaEditar" autocomplete="off" step="any" style="width:50%;margin-left:20%; border:0.5px dotted gray ; color: blue; text-align:center" disabled />
                </div>
            </div>
        </div>
    </div>
</div>

